{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "protos-admin-angular": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "css"
        }
      },
      "root": "",
      "sourceRoot": "src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": {
              "base": "dist/protos-admin-angular"
            },
            "index": "src/index.html",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.app.json",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "public"
              },
              {
                "glob": "**/*",
                "input": "node_modules/@fontsource-variable/inter/files",
                "output": "assets/fonts/inter"
              }
            ],
            "styles": [
              "node_modules/@aws-amplify/ui-angular/theme.css",
              "src/styles.css"
            ],
            "scripts": [],
            "extractLicenses": false,
            "polyfills": [
              "zone.js",
              "src/polyfills.ts" 
            ],
            "namedChunks": true,
            "browser": "src/main.ts",
            "allowedCommonJsDependencies": [
              "canvg",
              "html2canvas",
              "lodash",
              "qrcode",
              "fast-xml-parser",
              "crc-32",
              "raf",
              "rgbcolor",
              "buffer",
              "inflight",
              "stable",
              "source-map-url",
              "move-concurrently",
              "figgy-pudding",
              "urix",
              "har-validator",
              "lodash.template",
              "copy-concurrently",
              "consolidate",
              "resolve-url",
              "glob",
              "source-map-resolve",
              "q",
              "fs-write-stream-atomic",
              "mkdirp",
              "uuid",
              "webpack-chain",
              "request",
              "svgo",
              "docsearch.js",
              "highlight.js",
              "vue"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2MB",
                  "maximumError": "3MB"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "8kB",
                  "maximumError": "16kB"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "namedChunks": false,
              "extractLicenses": true,
              "allowedCommonJsDependencies": [
                "canvg",
                "html2canvas",
                "lodash",
                "qrcode",
                "fast-xml-parser",
                "crc-32",
                "raf",
                "rgbcolor",
                "buffer",
                "inflight",
                "stable",
                "source-map-url",
                "move-concurrently",
                "figgy-pudding",
                "urix",
                "har-validator",
                "lodash.template",
                "copy-concurrently",
                "consolidate",
                "resolve-url",
                "glob",
                "source-map-resolve",
                "q",
                "fs-write-stream-atomic",
                "mkdirp",
                "uuid",
                "webpack-chain",
                "request",
                "svgo",
                "docsearch.js",
                "highlight.js",
                "vue"
              ]
            },
            "development": {
              "optimization": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "protos-admin-angular:build:production"
            },
            "development": {
              "buildTarget": "protos-admin-angular:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "protos-admin-angular:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": ["zone.js"],
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "assets": [
              "src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/@fontsource-variable/inter/files",
                "output": "assets/fonts/inter"
              }
            ],
            "styles": [
              "node_modules/@aws-amplify/ui-angular/theme.css",
              "src/styles.css"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}